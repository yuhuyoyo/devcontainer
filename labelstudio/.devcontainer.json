{
  "name": "label studio",
  "image": "heartexlabs/label-studio:latest",
  "build": {
    "context": ".."
  },
  "service": "app",
  "shutdownAction": "none",
  "appPort": [
    "8080:8080"
  ],
  "runArgs": [
    "--name=application-server",
    "--network=app-network"
  ],
  "containerEnv": {
    "LABEL_STUDIO_USERNAME": "labelstudio@localhost",
    "LABEL_STUDIO_PASSWORD": "pwd"
  },
  "overrideCommand": false,
  "remoteUser": "root"
}
